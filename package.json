{
  "name": "poc-vuejs-project-alice",
  "version": "1.0.0",
  "description": "vue test project alice",
  "private": true,
  "scripts": {
    "test": "jest --coverage",
    "start:dev": "webpack-dev-server --open --hot",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint --ext .vue,.js --fix --cache src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escapedcat/poc-vuejs-project-alice.git"
  },
  "author": "escapedcat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/escapedcat/poc-vuejs-project-alice/issues"
  },
  "homepage": "https://github.com/escapedcat/poc-vuejs-project-alice#readme",
  "dependencies": {
    "vue": "2.5.16"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.13",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.11",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-vue": "4.4.0",
    "jest": "22.4.3",
    "jest-serializer-vue": "1.0.0",
    "node-sass": "4.8.3",
    "prettier-eslint": "8.8.1",
    "sass-loader": "6.0.7",
    "vue-jest": "2.3.0",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
