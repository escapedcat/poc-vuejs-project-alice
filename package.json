{
  "name": "poc-vuejs-project-alice",
  "version": "1.0.0",
  "description": "vue test project alice",
  "private": true,
  "scripts": {
    "test": "jest --coverage",
    "start:dev": "webpack-dev-server --open --hot",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint --ext .vue,.js --fix --cache src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escapedcat/poc-vuejs-project-alice.git"
  },
  "author": "escapedcat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/escapedcat/poc-vuejs-project-alice/issues"
  },
  "homepage": "https://github.com/escapedcat/poc-vuejs-project-alice#readme",
  "dependencies": {
    "vue": "2.5.16"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.14",
    "autoprefixer": "8.3.0",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.11",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-vue": "4.4.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "22.4.3",
    "jest-serializer-vue": "1.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.8.3",
    "postcss-loader": "2.1.4",
    "prettier-eslint": "8.8.1",
    "sass-loader": "7.0.1",
    "style-loader": "0.20.3",
    "vue-jest": "2.5.0",
    "vue-loader": "15.0.0-rc.2",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
